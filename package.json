{
  "name": "comissioflow",
  "version": "1.0.0",
  "private": true,
  "description": "Sistema de Comissionamento - Monorepo",
  "scripts": {
    "backend": "pnpm --filter backend dev",
    "frontend": "pnpm --filter frontend dev",
    "dev": "pnpm --parallel --filter backend --filter frontend dev",
    "build:backend": "pnpm --filter backend build",
    "build:frontend": "pnpm --filter frontend build",
    "build": "pnpm build:backend && pnpm build:frontend"
  },
  "keywords": ["commission", "sales", "management"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
